; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Sprite",
"class": "LayerSprite",
"language": "GDScript",
"path": "res://Scripts/Render/LayerSprite.gd"
}, {
"base": "LayerSprite",
"class": "SurfaceLayer",
"language": "GDScript",
"path": "res://Scripts/Render/Surface.gd"
}, {
"base": "Node2D",
"class": "WorldController",
"language": "GDScript",
"path": "res://Scripts/World/WorldController.gd"
}, {
"base": "Node",
"class": "WorldGen",
"language": "GDScript",
"path": "res://Scripts/World/WorldGen.gd"
}, {
"base": "Reference",
"class": "WorldMap",
"language": "GDScript",
"path": "res://Scripts/World/WorldMap.gd"
} ]
_global_script_class_icons={
"LayerSprite": "",
"SurfaceLayer": "",
"WorldController": "",
"WorldGen": "",
"WorldMap": ""
}

[application]

config/name="ProteanWorld"
run/main_scene="res://WorldCreation.tscn"
config/icon="res://icon.png"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false

[display]

window/size/height=768

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
